CREATE TABLE im_offline (
  "ID" integer PRIMARY KEY NOT NULL DEFAULT nextval('im_offiline_id') ,
  "PrincipalID" char(36) NOT NULL default '',
  "Message" text NOT NULL,
  "TMStamp" timestamp NOT NULL default now()
);

COMMIT;

